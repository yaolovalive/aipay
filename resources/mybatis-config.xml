<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- 设置全局运行行为 -->
	<settings>
		<!-- 设置日志系统为LOG4J -->
		<setting name="logImpl" value="LOG4J" />

		<!-- 全局cache开关:true(默认)/false -->
		<!-- <setting name="cacheEnable" value="false"/> -->

		<!-- resultMap映射级别:NONE:禁止自动匹配/PARTIAL:自动匹配(内嵌association,collection除外)/FULL:自动匹配所有 -->
		<!-- <setting name="autoMappingBehavior" value="NONE/PARTIAL(默认)/FULL"/> -->

		<!-- 全局性懒加载:true(默认)|false:初始化加载 -->
		<!-- <setting name="lazyLoadingEnabled" value="false"/> -->
	</settings>

	<typeAliases>
		<package name="com.yy.aipay.pojo" />
	</typeAliases>
</configuration>